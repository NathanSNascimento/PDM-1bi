            android:layout_height="24dp"
            android:id="@+id/text_frango" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="8dp"
            app:layout_constraintStart_toEndOf="@+id/textView8"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/text_lingui" android:gravity="center|center_horizontal"/>
    <TextView
            android:text="Frango:"
            android:layout_width="81dp"
            android:layout_height="24dp"
            android:id="@+id/textView8" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"
            android:textSize="18sp" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.076"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/textView6"
            android:gravity="right"/>
    <TextView
            android:text="Linguiça:"
            android:layout_width="81dp"
            android:layout_height="24dp"
            android:id="@+id/textView6"
            android:textSize="18sp"
            android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/textView4" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.076" android:gravity="right"/>
    <TextView
            android:layout_width="48dp"
            android:layout_height="24dp"
            android:id="@+id/text_lingui" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="8dp"
            android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/text_Maminha" app:layout_constraintStart_toEndOf="@+id/textView6"
            app:layout_constraintHorizontal_bias="0.0" android:gravity="center|center_horizontal"/>
    <TextView
            android:text="Maminha:"
            android:layout_width="81dp"
            android:layout_height="24dp"
            android:id="@+id/textView4"
            android:textSize="18sp"
            android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2" android:layout_marginStart="8dp"
