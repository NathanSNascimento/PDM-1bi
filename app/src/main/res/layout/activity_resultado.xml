    <TextView
            android:layout_width="81dp"
            android:layout_height="24dp"
            android:id="@+id/textView13"
            android:textSize="18sp"
            android:text="Farofa:"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/textView12"
            app:layout_constraintStart_toEndOf="@+id/text_lingui" android:layout_marginStart="60dp"
            android:layout_marginLeft="60dp"
            app:layout_constraintEnd_toStartOf="@+id/text_farofa"
            app:layout_constraintHorizontal_bias="1.0" android:gravity="right"/>
    <TextView
