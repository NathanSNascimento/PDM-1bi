            app:layout_constraintStart_toEndOf="@+id/textView4"
            app:layout_constraintHorizontal_bias="0.0"
            android:gravity="center|center_horizontal" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/text_Picanha"/>
