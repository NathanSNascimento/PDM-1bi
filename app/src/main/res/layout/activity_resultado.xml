    <TextView
            android:layout_width="48dp"
            android:layout_height="24dp"
            android:id="@+id/text_Picanha"
            android:layout_marginTop="44dp"
            app:layout_constraintTop_toBottomOf="@+id/textView" app:layout_constraintStart_toEndOf="@+id/textView2"
    />
    <TextView
            android:layout_width="81dp"
            android:layout_height="24dp"
            android:id="@+id/textView9"
            android:textSize="18sp"
            android:text="Arroz:" android:gravity="right"
            android:layout_marginTop="44dp"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            app:layout_constraintEnd_toStartOf="@+id/text_Arroz"
            app:layout_constraintStart_toEndOf="@+id/text_Picanha" android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp" app:layout_constraintHorizontal_bias="1.0"/>
    <TextView
            android:layout_width="81dp"
            android:layout_height="24dp"
            android:id="@+id/textView12"
            android:textSize="18sp"
            android:text="Batata:"
            android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/textView9" app:layout_constraintStart_toEndOf="@+id/text_Maminha"
