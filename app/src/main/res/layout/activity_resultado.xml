            android:layout_width="wrap_content"
            android:layout_height="143dp" app:srcCompat="@drawable/logo_alt"
            android:id="@+id/image_logo"
            app:layout_constraintTop_toTopOf="parent" android:scaleType="centerCrop"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="Ingredientes:"
            android:layout_width="0dp"
            android:layout_height="37dp"
            android:id="@+id/textView"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/image_logo" android:textSize="24sp"
            android:gravity="center_horizontal" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"/>
    <TextView
            android:text="Picanha:"
            android:layout_width="81dp"
            android:layout_height="24dp"
            android:id="@+id/textView2" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginTop="44dp"
            app:layout_constraintTop_toBottomOf="@+id/textView" android:textSize="18sp" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.076" android:gravity="right"/>
    <Button
            android:text="Voltar"
            android:layout_width="168dp"
            android:layout_height="wrap_content"
            android:id="@+id/button_voltar" android:layout_marginTop="84dp"
            app:layout_constraintTop_toBottomOf="@+id/textView14" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"/>

</android.support.constraint.ConstraintLayout>